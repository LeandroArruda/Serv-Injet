<!doctype html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>@ViewBag.Title</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="apple-touch-icon" href="apple-touch-icon.png">
     <link rel="apple-touch-icon" href="apple-touch-icon.png">

    <!-- Place favicon.ico in the root directory -->
    <!-- all css here -->
    <!-- bootstrap v3.3.6 css -->
    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <!-- animate css -->
    <link rel="stylesheet" href="/css/animate.css">
    <!-- jquery-ui.min css -->
    <link rel="stylesheet" href="/css/jquery-ui.min.css">
    <!-- meanmenu css -->
    <link rel="stylesheet" href="/css/meanmenu.min.css">
    <!-- owl.carousel css -->
    <link rel="stylesheet" href="/css/owl.carousel.css">
    <!-- font-awesome css -->
    <link rel="stylesheet" href="/css/font-awesome.min.css">
    <!-- Flaticons css -->
    <link rel="stylesheet" href="/css/flaticon.css">

    <!-- nivo slider CSS
    ============================================ -->
    <link rel="stylesheet" href="/inc/custom-slider/css/nivo-slider.css" type="text/css" />
    <link rel="stylesheet" href="/inc/custom-slider/css/preview.css" type="text/css" media="screen" />
    <!-- style css -->
    <link rel="stylesheet" href="/css/style.css">
    <!-- CSS Color Plate -->
    <link rel="stylesheet" href="/multicolor-css/skype-color.css">
    <link rel="stylesheet" href="/multicolor-css/red-color.css">
    <link rel="stylesheet" href="/multicolor-css/green-color.css">
    <link rel="stylesheet" href="/multicolor-css/blue-color.css">
    <!-- responsive css -->
    <link rel="stylesheet" href="/css/responsive.css">
    <!-- modernizr css -->
    <script src="/js/vendor/modernizr-2.8.3.min.js"></script>
    

    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-91423690-1', 'auto');
        ga('send', 'pageview');

    </script>
</head>

<body>
    <header>
        <div class="header-top-area">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                        <div class="header-top-left">
                            <ul>
                                <li><i class="fa fa-globe" aria-hidden="true"></i> Bem-vindo a Serv-Injet, mais de 30 anos de experiência.</li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                        <div class="header-top-right">
                            <ul>
                                <li><i class="fa fa-envelope-o" aria-hidden="true"></i> <a class="mail" href="mailto:contato@serv-injet.com.br" target="_top">contato@serv-injet.com.br</a> </li>
                                <li><i class="fa fa-phone" aria-hidden="true"></i> <a href="tel:15 99772 5959">(15) 99772-5959</a>  </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="main-header-area">
            <div class="container">
                <div class="row">
                    <div class="col-lg-2 col-md-2 col-sm-12 col-xs-12">
                        <div class="logo-area">
                            <a href='@Url.Action("Index", "Inicio")'><img src="/img/logo.png" alt="site logo"></a>
                        </div>
                    </div>
                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                        <div class="main-menu">
                            <ul>
                                <li class="active">@Html.ActionLink("Início", "Index", "Inicio")</li>
                                <li>@Html.ActionLink("Sobre", "Sobre", "Inicio")</li>
                                <li>@Html.ActionLink("Serviços", "Servico", "Inicio")</li>
                                <li>@Html.ActionLink("Projetos", "Projetos", "Inicio")</li>
                                <li>@Html.ActionLink("Contato", "Contato", "Inicio")</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="mobile-menu-area">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="mobile-menu">
                            <nav id="dropdown">
                                <ul>
                                    <li class="active">@Html.ActionLink("Início", "Index", "Inicio")</li>
                                    <li>@Html.ActionLink("Sobre", "Sobre", "Inicio")</li>
                                    <li>@Html.ActionLink("Serviços", "Servico", "Inicio")</li>
                                    <li>@Html.ActionLink("Projetos", "Projetos", "Inicio")</li>
                                    <li>@Html.ActionLink("Contato", "Contato", "Inicio")</li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>

    @RenderBody()


    <footer>
        <div class="footer-top-area">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center">
                        <div class="footer-top">
                            <p>Entre em contato</p>
                            <h2><i class="fa fa-phone" aria-hidden="true"></i><a class="actionMailPhone" href="tel:15 99772 5959"> (15) 99772-5959</a>  </h2>
                        </div>
                    </div>
                </div>
                <div class="row main-footer">
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                        <div class="single-footer">
                            <a href="index.html">
                                <img src="~/img/footer-logo.png" alt="">
                            </a>
                            <p>Empresa de prestação de serviços e fornecimento de peças para máquinas injetoras de plásticos e prensas</p>
                            <div class="footer-social-media-area">
                                <nav>
                                    <ul>
                                        <li><a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
                                        <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                                        <li><a href="#"><i class="fa fa-rss" aria-hidden="true"></i></a></li>
                                        <li><a href="#"><i class="fa fa-pinterest-p" aria-hidden="true"></i></a></li>
                                        <li><a href="#"><i class="fa fa-linkedin" aria-hidden="true"></i></a></li>
                                        <li><a href="#"><i class="fa fa-instagram"></i></a></li>
                                    </ul>
                                </nav>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-5 col-xs-12">
                        <div class="single-footer footer-two">
                            <h3>Serviços</h3>
                            <nav>
                                <ul>
                                    <li><i class="fa fa-arrow-circle-right" aria-hidden="true"></i>Assistência Técnica</li>
                                    <li><i class="fa fa-arrow-circle-right" aria-hidden="true"></i>Retrofit</li>
                                    <li><i class="fa fa-arrow-circle-right" aria-hidden="true"></i>Nacionalização parcial ou total da parte hidráulica e mecânica</li>
                                    <li><i class="fa fa-arrow-circle-right" aria-hidden="true"></i>Projetos de blocos hidráulicos</li>
                                    <li><i class="fa fa-arrow-circle-right" aria-hidden="true"></i>Desenvolvimento de equipamentos hidráulicos especiais sob encomenda</li>
                                    <li><i class="fa fa-arrow-circle-right" aria-hidden="true"></i>Elaboração de diagrama hidráulico sob encomenda</li>
                                    <li><i class="fa fa-arrow-circle-right" aria-hidden="true"></i>Atualização de equipamentos hidráulicos</li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-4 col-xs-12">
                        <div class="single-footer footer-three">
                            <h3>Escritório</h3>
                            <nav>
                                <ul>
                                    <li><i class="fa fa-paper-plane-o" aria-hidden="true"></i>Votorantim, SP</li>
                                    <li><i class="fa fa-phone" aria-hidden="true"></i> <a class="actionMailPhone" href="tel:15 99772 5959">(15) 99772-5959</a> </li>
                                    <li><i class="fa fa-envelope-o" aria-hidden="true"></i> <a class="actionMailPhone" href="mailto:contato@serv-injet.com.br" target="_top">contato@serv-injet.com.br</a></li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="footer-bottom-area">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center">
                        <p>&copy; Copyrights Serv-Injet. Todos os direitos reservados. 2017</p>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- footer area end here -->
    <!-- all js here -->
    <!-- jquery latest version -->
    <script src="/js/vendor/jquery-1.12.0.min.js"></script>
    <!-- bootstrap js -->
    <script src="/js/bootstrap.min.js"></script>
    <!-- owl.carousel js -->
    <script src="/js/owl.carousel.min.js"></script>
    <!-- meanmenu js -->
    <script src="/js/jquery.meanmenu.js"></script>
    <!-- jquery-ui js -->
    <script src="/js/jquery-ui.min.js"></script>
    <!-- wow js -->
    <script src="/js/wow.min.js"></script>
    <!-- jquery.counterup js -->
    <script src="/js/jquery.counterup.min.js"></script>
    <script src="/js/waypoints.min.js"></script>
    <!-- plugins js -->
    <script src="/js/plugins.js"></script>
    <!-- Nivo slider js
    ============================================ -->
    <script src="/inc/custom-slider/js/jquery.nivo.slider.js" type="text/javascript"></script>
    <script src="/inc/custom-slider/home.js" type="text/javascript"></script>
    <!-- main js -->
    <script src="/js/main.js"></script>
</body>
</html>
